{
  "name": "lh-metrics-analysis",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "test-all": "yarn lint && yarn type-check && yarn test",
    "test": "mocha test/**/*-test.js",
    "coverage": "c8 --all --include=js/**/*.js --reporter=html --reporter=lcovonly ./node_modules/.bin/mocha test/**/*-test.js",
    "lint": "eslint .",
    "type-check": "tsc -p ."
  },
  "engines": {
    "node": ">=14.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GoogleChromeLabs/lh-metrics-analysis.git"
  },
  "keywords": [
    "lighthouse",
    "httparchive"
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/GoogleChromeLabs/lh-metrics-analysis/issues"
  },
  "homepage": "https://github.com/GoogleChromeLabs/lh-metrics-analysis#readme",
  "dependencies": {
    "@google-cloud/bigquery": "^4.7.0",
    "@google-cloud/storage": "^5.0.1",
    "enquirer": "^2.3.5",
    "uuid": "^8.1.0"
  },
  "devDependencies": {
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.0.13",
    "@types/uuid": "^8.0.0",
    "babel-eslint": "^10.1.0",
    "c8": "^7.2.0",
    "eslint": "7.2.0",
    "eslint-config-google": "0.14.0",
    "mocha": "^8.0.1",
    "typescript": "^3.9.5"
  }
}
