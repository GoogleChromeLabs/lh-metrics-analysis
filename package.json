{
  "name": "lh-metrics-analysis",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "test-all": "yarn lint && yarn type-check && yarn test",
    "test": "mocha 'tests/**/*-test.js'",
    "coverage": "c8 --all --include={js,third_party}/**/*.js --exclude=third_party/boost/math/test/*.js --reporter=html --reporter=lcovonly ./node_modules/.bin/mocha 'tests/**/*-test.js'",
    "lint": "eslint .",
    "type-check": "tsc -p .",
    "build-action": "cd .github/actions/service-account-key/ && ncc build setup-service-account.js",
    "build-boost-special-functions": "rm -r c++/build; mkdir c++/build && cd c++/build && cmake .. && cmake --build ."
  },
  "engines": {
    "node": ">=14.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GoogleChromeLabs/lh-metrics-analysis.git"
  },
  "keywords": [
    "lighthouse",
    "httparchive"
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/GoogleChromeLabs/lh-metrics-analysis/issues"
  },
  "homepage": "https://github.com/GoogleChromeLabs/lh-metrics-analysis#readme",
  "dependencies": {
    "@google-cloud/bigquery": "^5.1.0",
    "@google-cloud/storage": "^5.2.0",
    "commander": "^6.0.0",
    "enquirer": "^2.3.5",
    "es-main": "^1.0.2",
    "uuid": "^8.3.0",
    "xorshift": "^1.1.1"
  },
  "devDependencies": {
    "@actions/core": "1.2.4",
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.0.27",
    "@types/uuid": "^8.3.0",
    "@zeit/ncc": "0.22.3",
    "babel-eslint": "^10.1.0",
    "c8": "^7.3.0",
    "csv-spectrum": "^1.0.0",
    "eslint": "7.2.0",
    "eslint-config-google": "0.14.0",
    "mocha": "^8.1.1",
    "typescript": "^3.9.7"
  }
}
